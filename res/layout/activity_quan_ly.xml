<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/TableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    
    android:orientation="vertical"
    android:paddingLeft="10dip"
    android:paddingRight="10dip" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:src="@drawable/ic_quanly" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="10dip"
        android:text="MANAGE"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textViewHoTen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin" 
        android:textSize="11sp"
        android:textColor="#3333CC"
        android:gravity="center"/>
    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11sp"
        android:text="Email" 
        android:textColor="#3333CC"
        android:gravity="center"/>
    <TextView
        android:id="@+id/textViewSdt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11sp"
        android:text="Số điện thoại:" 
        android:textColor="#3333CC"
        android:gravity="center"/>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <ImageButton
            android:id="@+id/buttonQLTinh"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:src="@drawable/ic_ql1"
            android:layout_column="0"
           	android:paddingLeft="25dip"
            android:layout_gravity="right"/>

        <ImageButton
            android:id="@+id/buttonQLHuyen"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingLeft="25dip"
            android:layout_column="1"
            android:layout_gravity="center"
            android:src="@drawable/ic_ql2"
            />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dip" >

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dip"
            android:text="Quản lý cấp tỉnh"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quản lý cấp huyện"
            android:paddingLeft="25dip"
            android:textAppearance="?android:attr/textAppearanceSmall" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <ImageButton
            android:id="@+id/buttonQLXa"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_ql3"
            android:paddingLeft="25dip"
            android:background="#ffffff"
             />

        <ImageButton
            android:id="@+id/buttonQLNguoiDung"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:paddingLeft="25dip"
            android:layout_column="1"
            android:layout_gravity="center"
            android:src="@drawable/ic_nd"
             />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quản lý cấp xã"
            android:paddingLeft="25dip"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quản lý người dùng"
            android:paddingLeft="30dip"
            android:layout_gravity="right"
            android:textAppearance="?android:attr/textAppearanceSmall" />

    </TableRow>

    <Button
        android:id="@+id/buttonDangXuat"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:text="Đăng xuất" />

    <Button
        android:id="@+id/buttonDoiMatKhau"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:text="Đổi mật khẩu" />

  

</TableLayout>